<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16sp" />
            <solid android:color="@color/material_dynamic_neutral_variant99" />
        </shape>
    </item>
    <item
        android:bottom="1sp"
        android:left="1sp"
        android:right="1sp"
        android:top="1sp">
        <shape android:shape="rectangle">
            <corners android:radius="16sp" />
            <stroke
                android:width="1sp"
                android:color="@color/appThemeC"
                android:dashWidth="4sp"
                android:dashGap="4sp" />
        </shape>
    </item>
</layer-list>
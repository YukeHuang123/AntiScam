<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <solid android:color="@color/material_dynamic_neutral_variant99" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <stroke
                android:width="1dp"
                android:color="@color/appThemeC"
                android:dashWidth="4dp"
                android:dashGap="4dp" />
        </shape>
    </item>
</layer-list>